{
    "name": "waterloobae/uw-adfs",
    "description": "University of Waterloo ADFS SAML authentication package for Laravel",
    "keywords": ["laravel", "adfs", "saml", "authentication", "uwaterloo"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "WaterlooBae",
            "email": "your-email@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "onelogin/php-saml": "^4.1"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "WaterlooBae\\UwAdfs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WaterlooBae\\UwAdfs\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "WaterlooBae\\UwAdfs\\UwAdfsServiceProvider"
            ],
            "aliases": {
                "UwAdfs": "WaterlooBae\\UwAdfs\\Facades\\UwAdfs"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}